<section class="marketplace-wrapper wrapper display-flex">
    {{> marketplace-side-nav}}
    <div class="marketplace-main">
        {{> marketplace-main-top-row}}
        <div class="marketplace-main-bottom-row">
            {{#if products}}
            {{#each products as |product|}}
            {{> product-card}}
            {{/each}}
            {{else}}
            <p>No item found</p>
            {{/if}}
        </div>
    </div>
</section>

<script src="/js/wishlist.js"></script>
<script src="/js/cart.js"></script>
<script src="/js/product.js"></script>